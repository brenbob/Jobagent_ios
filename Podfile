source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

target 'jobagent' do
  pod 'AFNetworking', '~> 3.0'
  pod 'GoogleAnalytics'
  pod 'Google-Mobile-Ads-SDK'

  use_frameworks!
  
  pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git'
end
